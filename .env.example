# Electron print tray configuration
# Copy this file to `.env` and adjust values as needed.

# Port the local API server listens on. Defaults to 19613 when unset.
PRINT_TRAY_PORT=19613

# Comma-separated list of allowed origins for CORS checks.
# e.g. http://localhost:3000,https://example.com
PRINT_TRAY_ALLOWED_ORIGINS=http://localhost,http://127.0.0.1

# Set to "true" to allow all origins (overrides PRINT_TRAY_ALLOWED_ORIGINS).
PRINT_TRAY_ALLOW_ALL_ORIGINS=false
